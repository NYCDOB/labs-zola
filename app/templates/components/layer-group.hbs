{{#if (and config resolved)}}
  {{log config}}
  {{#map.source 
    sourceId=config.id 
    options=config.options as |source|}}
    {{#each config.layers as |layerConfig|}}
      {{source.layer
        layer=layerConfig.layer
        before=layerConfig.before}}
    {{/each}}
  {{/map.source}}
{{/if}}
